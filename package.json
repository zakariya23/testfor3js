{
  "name": "threejs-canvas-sketch",
  "version": "1.0.0",
  "description": "A three.js scene running in canvas-sketch",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "start": "canvas-sketch src/index.js --hot --output=./output",
    "build": "canvas-sketch src/index.js --build --inline --name output --output=./output"
  },
  "dependencies": {
    "canvas-sketch": "0.7.4",
    "canvas-sketch-cli": "1.11.14",
    "canvas-sketch-util": "1.10.0",
    "dat.gui": "^0.7.7",
    "load-bmfont": "^1.4.1",
    "stats-js": "1.0.1",
    "three": "0.133.1",
    "three-bmfont-text": "^3.0.1",
    "three-csg": "^1.0.0",
    "troika-three-text": "^0.48.0"
  },
  "devDependencies": {
    "@types/three": "0.93.13"
  },
  "keywords": [
    "webgl",
    "three",
    "three.js",
    "canvas-sketch",
    "template",
    "threejs"
  ]
}
